<s:Envelope xmlns:a="http://www.w3.org/2005/08/addressing" xmlns:s="http://www.w3.org/2003/05/soap-envelope">
    <s:Header>
        <a:Action s:mustUnderstand="1">http://schemas.xmlsoap.org/ws/2004/09/enumeration/PullResponse</a:Action>
        <a:RelatesTo>{{MessageID}}</a:RelatesTo>
    </s:Header>
    <s:Body>
        <wsen:PullResponse xmlns:wsen="http://schemas.xmlsoap.org/ws/2004/09/enumeration" 
                           xmlns:addata="http://schemas.microsoft.com/2008/1/ActiveDirectory/Data">
            <wsen:Items>
                {% for obj in objects %}
                <addata:{{ obj.type }}>
                    {% for attr in obj.attrs %}
                    {{ attr.to_xml()|safe }}
                    {% endfor %}
                </addata:{{ obj.type }}>
                {% endfor %}
            </wsen:Items>
            {% if is_end %}
            <wsen:EndOfSequence />
            {% endif %}
        </wsen:PullResponse>
    </s:Body>
</s:Envelope>
