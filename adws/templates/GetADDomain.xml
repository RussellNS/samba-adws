<s:Envelope xmlns:s="http://www.w3.org/2003/05/soap-envelope" xmlns:a="http://www.w3.org/2005/08/addressing">
    <s:Header>
        <a:Action s:mustUnderstand="1">http://schemas.microsoft.com/2008/1/ActiveDirectory/CustomActions/TopologyManagement/GetADDomainResponse</a:Action>
        <a:RelatesTo>{{MessageID}}</a:RelatesTo>
    </s:Header>
    <s:Body>
        <GetADDomainResponse xmlns="http://schemas.microsoft.com/2008/1/ActiveDirectory/CustomActions/TopologyManagement"
                             xmlns:addata="http://schemas.microsoft.com/2008/1/ActiveDirectory/Data">
            <wsen:Items xmlns:wsen="http://schemas.xmlsoap.org/ws/2004/09/enumeration">
                {% for obj in objects %}
                <addata:domain>
                    {% for attr in obj.attrs %}
                    {{attr.to_xml()|safe}}
                    {% endfor %}
                </addata:domain>
                {% endfor %}
            </wsen:Items>
        </GetADDomainResponse>
    </s:Body>
</s:Envelope>
